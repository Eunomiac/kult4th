{{log "ACTOR INITIALIZATION" this}}

<form class="actor-sheet kult4th-form {{cssClass}} actor-pc pc-initialization" autocomplete="off">
  <div class="archetype-staging">
    <div class="archetype-carousel">
      {{#each archetypeCarousel}}
      <div class="archetype-carousel-item" data-index="{{@index}}" data-archetype="{{case "lower" label}}" data-is-selected="{{isSelected}}">
        <div class="archetype-carousel-the">The</div>
        <div class="archetype-carousel-name">{{label}}</div>
        <img class="archetype-carousel-img" src="{{img}}" alt="{{label}}">
        <div class="archetype-description">{{{description}}}</div>
      </div>
      <div class="archetype-carousel-panel" data-archetype="{{case "lower" label}}" data-is-selected="{{isSelected}}">
        <div class="panel-input-with-examples">
          <div class="archetype-input archetype-occupation content-editable clamp-text" data-field="flags.kult4th.{{case "lower" label}}.occupation" data-placeholder="Occupation" data-clamplines="1">{{occupation.value}}</div>
          <div class="archetype-input-examples">
            {{#each occupation.examples}}
              <div class="archetype-input-example" data-is-equal-to-value="{{test ../occupation.value "==" this}}">{{this}}</div>
            {{/each}}
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
  <div class="archetype-carousel-dragger"><div class="archetype-carousel-drag-handle"></div></div>
</form>