<div class="svg-container svg-{{key}} {{classes}}">
	{{#each (getSVGs key) as |svg|}}
	<svg class="svg-def {{~#if svg.classes}}{{#each svg.classes}} {{this}}{{/each}}{{/if}}" viewbox="{{svg.viewBox}}" {{~#if svg.style}} style="{{svg.style}}"{{/if}}>
		<path d="{{{svg.d}}}" />
	</svg>
	{{/each}}
</div>